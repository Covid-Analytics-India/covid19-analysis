  "components": {
    "schemas": {
      "countryWiseResponse": {
        "type": "object",
        "properties": {
          "x": {
            "type": "array",
            "items": {
              "format": "unix-epoch",
              "type" : "timestamp"
            },
            "example": [1589343248.00, 1523464560.00, 1589342148.00, 1590464560.00]
          },
          "y": {
            "type": "array",
            "items": {
              "format": "int64",
              "type" : "integer"
            },
             "example": [1, 2, 421, 12394]
          },
          "type": {
            "type": "string",
            "format": "string",
            "example": "line"
          },
          "title": {
            "type": "string",
            "format": "string",
            "example": "Title of the graph"
          },
          "x_label": {
            "type": "string",
            "format": "string"
          },
          "y_label": {
            "type": "string",
            "format": "string"
          }
        }
      },
      "travelHistoryResponseGraphData": {
        "type" : "object",
        "properties": {
          "values": {
            "type": "array",
            "items": {
              "format": "float",
              "type" : "float"
            },
            "example": [1.002, 2.123, 0.55, 90.23]
          },
          "labels": {
            "type": "array",
            "items": {
              "format": "string",
              "type": "string"
            },
            "example": ["Travelled from Delhi", "Travelled from Dubai"]
          },
          "type": {
            "type": "string",
            "format": "string",
            "example": "pie"
          },
          "title": {
            "type": "string",
            "format": "string",
            "example": "Travel history analysis"
          }
        }
      },
      "stateWiseResponse": {
        "type": "object",
        "properties": {
          "x": {
            "type": "array",
            "items": {
              "format": "float",
              "type" : "float"
            },
            "example": [1000, 1912, 3134,315]
          },
          "y": {
            "type": "array",
            "items": {
              "format": "string",
              "type" : "string"
            },
            "example": ["Maharashtra", "Madhya Pradesh", "Rajasthan", "Delhi"]
          },
          "orientation": {
            "type": "string",
            "format": "string",
            "example": "h"
          },
          "type": {
            "type": "string",
            "format": "string",
            "example": "bar"
          },
          "title": {
            "type": "string",
            "format": "string",
            "example": "Title of the graph"
          },
          "x_label": {
            "type": "string",
            "format": "string"
          },
          "y_label": {
            "type": "string",
            "format": "string"
          }
        }
      },
      "districtWiseResponse": {
        "type": "object",
        "properties": {
          "x": {
            "type": "array",
            "items": {
              "format": "float",
              "type" : "float"
            },
            "example": [1000, 1912, 3134,315]
          },
          "y": {
            "type": "array",
            "items": {
              "format": "string",
              "type" : "string"
            },
            "example": ["Mumbai", "Indore", "Kota", "Rajgarh"]
          },
          "orientation": {
            "type": "string",
            "format": "string",
            "example": "h"
          },
          "type": {
            "type": "string",
            "format": "string",
            "example": "bar"
          },
          "title": {
            "type": "string",
            "format": "string",
            "example": "Title of the graph"
          },
          "x_label": {
            "type": "string",
            "format": "string"
          },
          "y_label": {
            "type": "string",
            "format": "string"
          }
        }
      },
      "getAll": {
        "type": "object",
        "properties": {
          "country_wise": {
            "type": "object",
            "properties": {
              "day_wise_confirmed": {
                "type": "object,",
                "$ref": "#/components/schemas/countryWiseResponse"
              },
              "day_wise_encountered": {
                "type": "object",
                "$ref": "#/components/schemas/countryWiseResponse"
              },
              "recovered_cumulative":{
                "type": "object",
                "$ref": "#/components/schemas/countryWiseResponse"
              },
              "day_wise_recovered": {
                "type": "object",
                "$ref": "#/components/schemas/countryWiseResponse"
              },
              "deaths_cumulative": {
                "type": "object",
                "$ref": "#/components/schemas/countryWiseResponse"
              },
              "day_wise_deaths": {
                "type": "object",
                "$ref": "#/components/schemas/countryWiseResponse"
              }
            }
          },
          "state_wise": {
            "type": "object",
            "properties": {
              "state_wise_confirmed": {
                "type": "object",
                "$ref": "#/components/schemas/stateWiseResponse"
              },
              "state_wise_recovered": {
                "type": "object",
                "$ref": "#/components/schemas/stateWiseResponse"
              },
              "state_wise_deaths": {
                "type": "object",
                "$ref": "#/components/schemas/stateWiseResponse"
              },
              "state_codes": {
                "type": "array",
                "items": {
                  "format": "string",
                  "type": "string"
                },
                "example": ["MH", "MP", "RJ", "DL"]
              }
            }
          },
          "district_wise": {
            "type": "object",
            "properties": {
              "district_wise_confirmed": {
                "type": "object",
                "$ref": "#/components/schemas/districtWiseResponse"
              },
              "district_wise_active": {
                "type": "object",
                "$ref": "#/components/schemas/districtWiseResponse"
              },
              "district_wise_recovered": {
                "type": "object",
                "$ref": "#/components/schemas/districtWiseResponse"
              },
              "district_wise_deaths": {
                "type": "object",
                "$ref": "#/components/schemas/districtWiseResponse"
              }
            }
          },
          "travel_history_analysis": {
            "type": "object",
            "$ref" :  "#/components/schemas/travelHistoryResponseGraphData"
          }
        }
      }
    }
  }
}
